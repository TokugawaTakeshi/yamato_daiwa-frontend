    "@yamato-daiwa/frontend-documenting_kit": "../DocumentingKit/Package",
    "@yamato-daiwa/programming_documentation": "../../YamatoDaiwaProgrammingDocumentation",
    "clipboard": "2.0.11",
    "prismjs": "1.29.0"
  },
