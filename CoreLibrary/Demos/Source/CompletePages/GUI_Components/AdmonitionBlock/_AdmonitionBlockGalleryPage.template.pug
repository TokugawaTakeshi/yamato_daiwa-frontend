    /**
     * @param { string } [partialTitle]
     * @returns { string }
     * */
    function generateHTML_PageTitle(partialTitle) {
      return [
        "AdmonitionBlock Component Demos",
        ...partialTitle ? [ ` — ${ partialTitle }` ] : [],
        " — Yamato Daiwa Frontend (YDF)"
      ].join("");
    }

    RegularWebPageTemplate__YDF.configure({
      stylesSheetsURIs: [
        "@Partials/YDF-Demos-Primer",
        "@Partials/YDF-Demos-CommonRequirements",
        "@Pages/GUI_Components/AdmonitionBlock/AdmonitionBlockGalleryPage"
      ],
      scriptsURIs: {
        atEndOfBody: [ "@CompletePages/GUI_Components/AdmonitionBlock/AdmonitionBlockGalleryPage" ]
      }
    });


